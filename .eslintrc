{
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "env": {
        "node": true,
        "mocha": true
    },
    "rules": {
        "no-console": "off",
        "no-underscore-dangle": "off",
        "no-return-assign": "off",
        "consistent-return": "off",
        "no-use-before-define": "off",
        "no-param-reassign": "off",
        "camelcase": "off",
        "no-self-assign": "off",
        "no-unused-vars": "warn",
        "import/no-extraneous-dependencies": "off",
        "func-names": "off",
        "prettier/prettier": [
            "warn",
            {
                "endOfLine": "auto"
            }
        ]
    }
}
